{
  "version": 2,
  "builds": [
    {
      "src": "dataBiomasse/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build", 
      "config": {
          "distDir": "static"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dataBiomasse/wsgi.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "true",
    "DJANGO_SETTINGS_MODULE": "dataBiomasse.settings"
  }
}

  